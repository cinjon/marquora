{
  "name": "My First Extension",
  "version": "1.0",
  "description": "The first extension that I made.",
  "background_page": "background.html",
  "browser_action": {
    "default_icon": "icon.png",
	"default_name": "Page Language",
	"popup": "popup.html"
  },
  "permissions": [
	"tabs", "http://*/*", "https://*/*"
  ],
  "content_scripts": [
    {
	  "matches": [ "http://*.quora.com/*" ],
	  "js": ["contentscript.js"]
	}
  ]
}